{
  "extends": ["tslint:latest", "tslint-config-prettier"],
  "rules": {
    "curly": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "member-access": false,
    "member-ordering": [true, {
      "order": [
        "private-instance-field",
        "protected-instance-field",
        "public-instance-field"
      ]
    }]
  }
}